#include "main.h"
/**
 * _strcpy - copies the string to the buffer pointed
 * @dest: This is destiny
 * @src: This is the copied item
 *
 * Return: return copy item
 */
char *_strcpy(char *dest, char *src)
{
	char *start = dest;

	while (*src != '\0')
	{
		*dest = *src;
		dest++;
		src++;
	}
	*dest = '\0';
	return (start);
}
